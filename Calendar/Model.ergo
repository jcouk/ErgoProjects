// schema


Year
[|
	name => \string
|].

Month
[|
	name => \string,
	year => Year,
	monthNum => \integer
	
|].

Day
[|
	name => \string,
	totalHourNum => \string,
	month => Month,
	dayNum => \integer
|].

Hour
[|
	number => \double,
	day => Day
|].

Minute
[|
	hour => Hour
|].


